{
  "Routes": [
    {
      "UpstreamPathTemplate": "/api/users/{everything}",
      "UpstreamHttpMethod": [ "GET", "POST", "PUT", "DELETE" ],
      "DownstreamPathTemplate": "/api/users/{everything}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5001
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/shorten",
      "UpstreamHttpMethod": [ "POST" ],
      "DownstreamPathTemplate": "/api/shorten",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "shortener-service.railway.internal",
          "Port": 8080
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/api/urls/{everything}",
      "UpstreamHttpMethod": [ "GET", "PUT", "DELETE" ],
      "DownstreamPathTemplate": "/api/urls/{everything}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "shortener-service.railway.internal",
          "Port": 8080
        }
      ]
    },
    {
      "UpstreamPathTemplate": "/{shortCode}",
      "UpstreamHttpMethod": [ "GET" ],
      "DownstreamPathTemplate": "/{shortCode}",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "shortener-service.railway.internal",
          "Port": 8080
        }
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "https://api-gateway-production.up.railway.app"
  }
}
